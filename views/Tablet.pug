extends layout

block content
  h1= title
  - var results = [{Tablet_type: 2.3,composition: "Android",Tablet_features:"LED"},{Tablet_type: 2.5,composition: "IOS",Tablet_features:"LCD"},{Tablet_type: 3.2,composition: "LINUX",Tablet_features:"LMD"}];
   div(class = "container-fluid")
    div(class = "row")
     each form in results
        div(class="col-sm-4") Tablet-type: #{form.Tablet_type} 
        br
        br 
        div(class="col-sm-4") composition: #{form.composition}
        br
        br  
        div(class="col-sm-4") Tablet_features: #{form.Tablet_features}